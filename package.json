{
  "name": "memcached",
  "version": "3.0.0",
  "author": "Arnout Kazemier",
  "description": "A fully featured Memcached API client, supporting both single and clustered Memcached servers through consistent hashing and failover/failure. Memcached is rewrite of nMemcached, which will be deprecated in the near future.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib test",
    "clean-all": "rimraf dist node_modules package-lock.json",
    "lint": "tslint --fix './src/**/*.ts'",
    "docker": "docker-compose up --force-recreate &",
    "docker:kill": "docker-compose kill",
    "prebuild": "npm run clean",
    "build": "npm run lint && tsc",
    "pretest": "npm run build",
    "test": "nyc mocha --opts mocha.opts",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "InnoDB memcached API",
    "cache",
    "client",
    "cluster",
    "failover",
    "hashing",
    "membase",
    "memcache",
    "memcached",
    "nMemcached",
    "nosql"
  ],
  "bugs": {
    "url": "https://github.com/3rd-Eden/memcached/issues"
  },
  "engines": {
    "node": ">=6.14.4"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/3rd-Eden/memcached.git"
  },
  "dependencies": {
    "hashring": "^3.2.0",
    "jackpot": "^0.0.6"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.10.2",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "3.0.x"
  }
}
