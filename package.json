  },
  "repository": {
    "type": "git",
    "url": "http://github.com/3rd-Eden/node-memcached.git"
  },
  "dependencies": {
    "hashring": "0.0.x",
    "jackpot": ">=0.0.6"
  },
  "devDependencies": {
